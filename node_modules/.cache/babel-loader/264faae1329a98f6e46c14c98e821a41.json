 <li class=\"cart-list-con5\">\n            <a @click=\"decNum(index)\" class=\"mins\">-</a>\n            <input\n              autocomplete=\"off\"\n              type=\"text\"\n              :value=\"shopitem.skuNum\"\n              minnum=\"1\"\n              class=\"itxt\"\n              @change=\"changeNum(index)\"\n            />\n            <a @click=\"addNum(index)\" class=\"plus\">+</a>\n          </li>\n          <li class=\"cart-list-con6\">\n            <span class=\"sum\">{{ shopitem.skuPrice * shopitem.skuNum }}</span>\n          </li>\n          <li class=\"cart-list-con7\">\n            <a class=\"sindelet\" @click=\"deleteitem(index)\">删除</a>\n            <br />\n            <a href=\"#none\">移到收藏</a>\n          </li>\n        </ul>\n      </div>\n    </div>\n    <div class=\"cart-tool\">\n      <div class=\"select-all\">\n        <input class=\"chooseAll\" type=\"checkbox\" />\n        <span>全选</span>\n      </div>\n      <div class=\"option\">\n        <a href=\"#none\">删除选中的商品</a>\n        <a href=\"#none\">移到我的关注</a>\n        <a href=\"#none\">清除下柜商品</a>\n      </div>\n      <div class=\"money-box\">\n        <div class=\"chosed\">已选择 <span>0</span>件商品</div>\n        <div class=\"sumprice\">\n          <em>总价（不含运费） ：</em>\n          <i class=\"summoney\">{{}}</i>\n        </div>\n        <div class=\"sumbtn\">\n          <a class=\"sum-btn\" href=\"###\" target=\"_blank\">结算</a>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex'\nexport default {\n  name: 'ShopCart',\n  data() {\n    return {}\n  },\n  mounted() {\n    this.getData()\n  },\n  methods: {\n    getData() {\n      this.$store.dispatch('reqGetShopList')\n    },\n    getNum(index, skuNum) {\n      this.$store.dispatch('reqGetaddToCart', {\n        skuId: this.shopList[index].skuId,\n        skuNum: skuNum,\n      })\n    },\n    changeNum(index) {\n      let originvalue = this.shopList[index].skuNum\n      let value = event.target.value\n      if (isNaN(value) || value < 1) {\n        value = 1\n      } else {\n        value = parseInt(value)\n      }\n      let changeNum = value - originvalue\n      this.getNum(index, changeNum)\n      this.shopList[index].skuNum = value\n    },\n    addNum(index) {\n      this.shopList[index].skuNum++\n      this.getNum(index, 1)\n    },\n    decNum(index) {\n      this.shopList[index].skuNum--\n      this.getNum(index, -1)\n    },\n    deleteitem(index) {\n      this.$store.dispatch('reqGetdeleteCart', {\n        skuId: this.shopList[index].skuId,\n      })\n      this.shopList.splice(index, 1)\n    },\n    isclick(event) {\n      // console.log(event)\n    },\n  },\n  computed: {\n    ...mapState({\n      shopList: (state) => state.shopCart.shopList,\n    }),\n  },\n}\n</script>\n\n<style lang=\"less\" scoped>\n.cart {\n  width: 1200px;\n  margin: 0 auto;\n\n  h4 {\n    margin: 9px 0;\n    font-size: 14px;\n    line-height: 21px;\n  }\n\n  .cart-main {\n    .cart-th {\n      background: #f5f5f5;\n      border: 1px solid #ddd;\n      padding: 10px;\n      overflow: hidden;\n\n      & > div {\n        float: left;\n      }\n\n      .cart-th1 {\n        width: 25%;\n\n        input {\n          vertical-align: middle;\n        }\n\n        span {\n          vertical-align: middle;\n        }\n      }\n\n      .cart-th2 {\n        width: 25%;\n      }\n\n      .cart-th3,\n      .cart-th4,\n      .cart-th5,\n      .cart-th6 {\n        width: 12.5%;\n      }\n    }\n\n    .cart-body {\n      margin: 15px 0;\n      border: 1px solid #ddd;\n\n      .cart-list {\n        padding: 10px;\n        border-bottom: 1px solid #ddd;\n        overflow: hidden;\n\n        & > li {\n          float: left;\n        }\n\n        .cart-list-con1 {\n          width: 15%;\n        }\n\n        .cart-list-con2 {\n          width: 35%;\n\n          img {\n            width: 82px;\n            height: 82px;\n            float: left;\n          }\n\n          .item-msg {\n     